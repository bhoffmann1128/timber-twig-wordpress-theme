{% if menu %}
	<ul>
	{% for item in items %}
		<li class="{{ item.classes | join(' ') }}">
			{% set targetLink = item.placeholder_page ? "#" : item.link %}
			<a target="{{item.target}}" href="{{ targetLink }}" >{{ item.title }}</a>
			{% if item.children %}
				<button class="mobile__toggle">+</button>
				<div class="nav__dropdown">
					{% include "menu.twig" with {'items': item.children} %}
				</div>
			{% endif %}
		</li>
	{% endfor %}
	</ul>
{% endif %}
